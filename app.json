{
  "expo": {
    "name": "Habitful",
    "slug": "habitful",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/loadingScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anamark.habitful",
      "buildNumber": "1.0.5"
    },
    "android": {
      "package": "com.anamark.habitful",
      "icon": "./assets/logo.png"
    },
    "web": {
      "build": {
        "babel": {
          "include": ["galio-framework"]
        }
      }
    }
  }
}
