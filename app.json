{
  "expo": {
    "name": "Habit Tracker 1.0.2",
    "slug": "habit-tracker-two",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/loadingScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anamark.appname",
      "buildNumber": "1.0.2"
    },
    "android": {
      "package": "com.anamark.appname"
    },
    "web": {
      "build": {
        "babel": {
          "include": ["galio-framework"]
        }
      }
    }
  }
}
